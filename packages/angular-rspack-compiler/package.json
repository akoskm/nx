  },
  "devDependencies": {
    "jsonc-eslint-parser": "^2.4.0",
    "vitest": "^1.3.1"
  },
  "peerDependencies": {
    "@angular/compiler-cli": ">=19.0.0 <20.0.0",
    "@angular/build": ">=19.0.0 <20.0.0",
    "@rsbuild/core": ">=1.0.5 <2.0.0"
